{
  "name": "plastikaweb2020-cms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p start:ng gql:codegen:watch",
    "start:ng": "ng serve",
    "open": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer --aot --optimization --vendor-chunk",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "lint:type": "ng lint --type-check",
    "lint:fix": "ng lint --fix",
    "stylish": "ng lint --format stylish",
    "e2e": "ng e2e",
    "clean": "rimraf node_modules",
    "clean:all": "rimraf node_modules dist dll && npm cache clean --force",
    "clean:dist": "rimraf dist",
    "reinstall": "yarn clean:all -- && rimraf package-lock.json && yarn",
    "gql:update-schema": "graphql get-schema -e default",
    "gql:codegen": "graphql-codegen --config codegen.yml",
    "gql:codegen:watch": "graphql-codegen --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "hammerjs": "^2.0.8",
    "rimraf": "^3.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.17",
    "@angular/cli": "~8.3.17",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.13",
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-apollo-angular": "1.8.3",
    "@graphql-codegen/typescript-operations": "1.8.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "graphql-cli": "^3.0.14",
    "husky": "^3.0.9",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "markdownlint": "^0.17.2",
    "markdownlint-cli": "^0.19.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "run-s test:single build:prod",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
